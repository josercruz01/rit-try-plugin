<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Try Plugins"
            id="com.plugin.tryplugin">
      </category>
      <view
            name="try plugin"
            icon="icons/rit.jpg"
            category="com.plugin.tryplugin"
            class="com.plugin.tryplugin.views.SampleView"
            id="com.plugin.tryplugin.views.SampleView">
      </view>
      <view
            name="try plugin main view"
            icon="icons/rit.jpg"
            category="com.plugin.tryplugin"
            class="com.plugin.tryplugin.views.TryPluginMainView"
            id="com.plugin.tryplugin.views.TryPluginMainView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="com.plugin.tryplugin.views.TryPluginMainView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
